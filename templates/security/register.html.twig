{% trans_default_domain "OurTripsTrans" %}
{% extends "base.html.twig" %}
{% block title %}
    {{ "title.register"|trans }}
{% endblock %}
{% block body %}
    <div class="login">
        {{ form_start(form) }}
        {{ form_row(form.firstname) }}
        {{ form_row(form.lastname) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_end(form) }}
    </div>
{% endblock %}


